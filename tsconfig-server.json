{
    "compilerOptions": {
        "module": "commonjs",
        "moduleResolution": "node",
        "target": "es5",
        "sourceMap": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "noImplicitAny": false,
        "outDir": "build/server/",
        "rootDir": "server"
    },
    "exclude": [
	    "node_modules",
      "typings"
    ],
    "include": [
      "server/**/*.ts",
      "typings/main",
      "typings/index.d.ts"
    ]
}
