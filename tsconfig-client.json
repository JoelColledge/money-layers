{
    "compilerOptions": {
        "module": "commonjs",
        "moduleResolution": "node",
        "target": "es5",
        "sourceMap": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "noImplicitAny": false,
        "outDir": "build/client/",
        "rootDir": "client"
    },
    "exclude": [
	    "node_modules"
    ],
    "include": [
      "client/**/*.ts",
      "typings/main",
      "typings/index.d.ts"
    ]
}
