{
  "name": "money-layers",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "start": "npm run start-server",
    "postinstall": "npm run build-production",
    "start-client": "ng serve --proxy-config proxy.conf.json",
    "build-server": "gulp server.build_all",
    "start-server": "node index",
    "dev": "npm run build-server && concurrently \"npm run start-server\" \"npm run start-client\"",
    "build-production": "rm -rf build/ dist/ && npm run build-server && ng build",
    "assemble-production": "npm run build-production && npm pack"
  },
